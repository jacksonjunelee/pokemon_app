      if (data[1][j].api_ref == i) {
        var dexTag = $('<p>').attr("id",[i]).text(i + '| ' + data[1][j].name);
        dexTag.appendTo('body');
      }


      else {
        var dexTag = $('<p>').attr("id",[i]).text(i + '| '  + '------------');
        dexTag.appendTo('body');
      }
    }
  }
});

</script>


